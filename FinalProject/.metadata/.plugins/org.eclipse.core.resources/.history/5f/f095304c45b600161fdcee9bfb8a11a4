import csv
with open('C:/~/Documents/GitHub/ANLY_520-51_FA2016/FinalProject/data/EmotionText.csv') as csvfile:
    reader=csv.DictReader(csvfile, dialect='excel')
    for row in reader:
        print(row['sentiment'],row['author'],row['content'])
