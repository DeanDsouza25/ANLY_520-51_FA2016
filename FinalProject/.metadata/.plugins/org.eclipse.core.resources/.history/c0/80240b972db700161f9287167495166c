import re

def pToken(tok):
    temp = []
    for t in tok:
        if re.search('^[A-Za-z0-9]+\.\.+[A-Za-z0-9]*$',t):
            # i = t.index('.')
            j = t.rfind('.')
            temp.append(t[:j])
            temp.append(t[j:])
        elif re.search('http://[A-Za-z0-9\./]+$',t):
            temp.append("L1")
        elif re.search('^[A-Za-z0-9]+\.[A-Za-z0-9]+$',t):
            temp.append("L1")
        else:
            temp.append(t)
            
    return temp

var = raw_input("Enter the string :")
varTok = var.split(' ')
tempList = pToken(varTok)
for x in tempList:
    print x
