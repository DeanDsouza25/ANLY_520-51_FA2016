import re

def pToken(psent):
    tok = psent.split(' ')
    temp = []
    for t in tok:
        if re.search('^[A-Za-z0-9]+\.\.+[A-Za-z0-9]*.*$',t):
            # i = t.index('.')
            j = t.rfind('.')
            temp.append(t[:j])
            temp.append(t[j+1:])
        elif re.search('http://[A-Za-z0-9\./]+$',t):
            temp.append("L1NK")
        elif re.search('^[A-Za-z0-9]+\.[A-Za-z0-9]+$',t):
            temp.append("L1NK")
        else:
            temp.append(t)
    return temp

var = raw_input("Enter the string :")
tempList = pToken(var)
for x in tempList:
    print x
