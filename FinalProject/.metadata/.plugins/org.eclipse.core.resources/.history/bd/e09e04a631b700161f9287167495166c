import re

def pToken(psent):
    tok = psent.split(' ')
    temp = []
    for t in tok:
        if re.search('^[A-Za-z0-9]+\.\.+[A-Za-z0-9]*.*$',t):
            i = t.index('.')
            j = t.rfind('.')
            temp.append(t[:i])
            temp.append(t[i:j+1])
            for x in re.findall('[A-Za-z0-9]+',t[j:]):
                temp.append(x)
            for x in re.findall('.+',t[j:]):
                temp.append(x)
        elif re.search('http://[A-Za-z0-9\./]+$',t):
            temp.append("L1NK")
        elif re.search('^[A-Za-z0-9]+\.[A-Za-z0-9]+$',t):
            temp.append("L1NK")
        # elif re.search('^[A-Za-z0-9]+.+$',t):
        #    for x in re.findall('[A-Za-z0-9]+',t):
        #        temp.append(x)
        #    for x in re.findall('.+',t):
        #        temp.append(x)
        else:
            temp.append(t)
    return temp

var = raw_input("Enter the string :")
tempList = pToken(var)
for x in tempList:
    print x
