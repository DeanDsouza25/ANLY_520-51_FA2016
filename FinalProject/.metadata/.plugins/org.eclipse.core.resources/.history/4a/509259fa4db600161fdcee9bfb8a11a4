import csv
import nltk

TempSt = ''

csvfile = open('C:/Users/demon/OneDrive/Documents/GitHub/ANLY_520-51_FA2016/FinalProject/data/EmotionText.csv')
nfile = open('C:/Users/demon/OneDrive/Documents/GitHub/ANLY_520-51_FA2016/FinalProject/data/AllText.txt','w')
reader = csv.DictReader(csvfile, dialect='excel')
for row in reader:
    nfile.write(row['content']+r'\n')

nfile.close()

nfile = open('C:/Users/demon/OneDrive/Documents/GitHub/ANLY_520-51_FA2016/FinalProject/data/AllText.txt','r')
nfile.readline()